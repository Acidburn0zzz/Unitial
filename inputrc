#bind key
"\e[1~": beginning-of-line
"\e[1~":  beginning-of-line
"\e[2~": overwrite-mode
"\e[3~": delete-char
"\e[4~": end-of-line
"\e[5C~": forward-word
"\e[5D~": backward-word

#bash tab auto complete
"\C-i": menu-complete
set show-all-if-ambiguous on
set mark-symlinked-directories on
set completion-ignore-case on

